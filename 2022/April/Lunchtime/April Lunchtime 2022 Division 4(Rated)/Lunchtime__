import java.util.*;
import java.lang.*;
import java.io.*;
// madhvender coding!

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int t= sc.nextInt();
		while(t--!=0) {
		   int x=sc.nextInt();
		   if(x>=1 && x<=4) {
		      System.out.println("YES");
		   }
		   else {
		      System.out.println("NO");
		   }
		    
		    
		    
		    
		}
	}
}
