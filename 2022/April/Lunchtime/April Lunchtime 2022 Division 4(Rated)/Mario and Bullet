import java.util.*;
import java.lang.*;
import java.io.*;
// madhvender coding!

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int t= sc.nextInt();
		while(t--!=0) {
		   int x=sc.nextInt(); int y=sc.nextInt(); int z=sc.nextInt();
		   int diff=z-(y/x);
		   if(diff<0) {
		      System.out.println(0);
		   }
		   else {
		      System.out.println(diff);
		   }
		   
		    
		    
		    
		    
		}
	}
}
